{
    "name": "compasshb/compasshb.com",
    "description": "compasshb.com",
    "repositories": [
        {
            "type": "composer",
            "url": "http://wpackagist.org"
        }
    ],
    "require": {
        "php": "~5.4",
        "johnpbloch/wordpress": "~4.1",
        "bugsnag/bugsnag": "2.*",
        "vimeo/vimeo-api": "1.*",
        "vlucas/phpdotenv": "1.*",
        "guzzlehttp/guzzle": "~5.1",
        "wpackagist-plugin/disqus-comment-system": "~2.84",
        "wpackagist-plugin/feedpress": "~1.6",
        "wpackagist-plugin/jetpack": "3.*",
        "wpackagist-plugin/varnish-http-purge": "~3.6",
        "wpackagist-plugin/vaultpress": "~1.7",
        "wpackagist-plugin/w3-total-cache": "0.9.4.1",
        "wpackagist-plugin/wordpress-seo": "~1.7",
        "wpackagist-plugin/simple-history": "~2.0",
        "wpackagist-plugin/restrict-categories": "~2.6",
        "wpackagist-plugin/remove-category-url": "~0.1",
        "wpackagist-plugin/jetpack": "~3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "wp-cli/wp-cli": "0.17.1"
    },
    "autoload": {
        "psr-4": {
            "CompassHB\\": "src/"
        }
    },
    "extra": {
        "wordpress-install-dir": "web/wp",
        "installer-paths": {
            "web/app/plugins/{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    }
}